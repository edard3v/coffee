---
import Layout from "@layouts/Layout/Layout.astro";
import { ROUTER } from "@router/router";
import Banner from "./Banner/Banner.astro";
import CoffeeExperience from "./CoffeeExperience/CoffeeExperience.astro";
import Banner2 from "./Banner2/Banner2.astro";
import Event from "./Event/Event.astro";
import Galery from "./Galery/Galery.astro";
---

<Layout title={ROUTER.home.display}>
  <main class="home">
    <Banner />
    <CoffeeExperience />
    <Banner2 />
    <Event />
    <Galery />
  </main>
</Layout>

<style>
  .home {
    padding: 0;
  }
</style>
